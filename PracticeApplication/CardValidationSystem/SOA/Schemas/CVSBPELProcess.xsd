<?xml version="1.0" encoding="UTF-8"?> 
<schema attributeFormDefault="unqualified"
	elementFormDefault="qualified"
	targetNamespace="http://xmlns.oracle.com/PracticeApplication/CardValidationSystem/CVSBPELProcess"
	xmlns="http://www.w3.org/2001/XMLSchema">
	<element name="cardReq">
		<complexType>
			<sequence>
				<element name="ccNumber" type="integer"/>
    <element name="ccVendor" type="string"/>
   </sequence>
		</complexType>
	</element>
	<element name="cardRes">
		<complexType>
			<sequence>
                        			<element name="ccNumber" type="integer"/>
    <element name="ccVendor" type="string"/>
				<element name="result" type="string"/>
   </sequence>
		</complexType>
	</element>
</schema>